public class RandomMethodDemo
{
    public static void main(String [] args)
    {
        double randomNumber;
        int counter = 0;

        while(counter < 10)
        {
            randomNumber = Math.random();
            System.out.println(counter + ":  " + randomNumber);
            counter ++;
        }//end of while loop
    }//end of main method
}//end of class
